{
  "name": "MNSwiftKit",
  "version": "0.1.0",
  "summary": "Swift开发基础组件。",
  "description": "Swift开发基础组件，模块化。",
  "homepage": "https://github.com/panhub/MNSwiftKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "panhub": "fengpann@163.com"
  },
  "source": {
    "git": "https://github.com/panhub/MNSwiftKit.git",
    "tag": "0.1.0"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "Full",
  "subspecs": [
    {
      "name": "Core",
      "frameworks": [
        "Foundation",
        "CoreFoundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Core/Classes/**/*.{swift}"
    },
    {
      "name": "Base",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics",
        "QuartzCore",
        "ImageIO",
        "CoreServices",
        "UniformTypeIdentifiers"
      ],
      "source_files": "MNSwiftKit/Base/Classes/**/*.{swift}"
    },
    {
      "name": "Database",
      "frameworks": [
        "Foundation",
        "AVFoundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Database/Classes/**/*.{swift}"
    },
    {
      "name": "Full",
      "dependencies": {
        "MNSwiftKit/Core": [],
        "MNSwiftKit/Base": [],
        "MNSwiftKit/Database": []
      }
    }
  ],
  "swift_version": "5.0"
}
