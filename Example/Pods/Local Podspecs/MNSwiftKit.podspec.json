{
  "name": "MNSwiftKit",
  "version": "0.1.0",
  "summary": "Swift开发基础组件。",
  "description": "Swift开发基础组件，模块化。",
  "homepage": "https://github.com/panhub/MNSwiftKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "panhub": "fengpann@163.com"
  },
  "source": {
    "git": "https://github.com/panhub/MNSwiftKit.git",
    "tag": "0.1.0"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "Full",
  "subspecs": [
    {
      "name": "UI",
      "frameworks": [
        "UIKit",
        "Foundation",
        "AVFoundation",
        "CoreGraphics",
        "QuartzCore",
        "ImageIO",
        "CoreServices",
        "UniformTypeIdentifiers"
      ],
      "source_files": "MNSwiftKit/UI/Sources/**/*.swift",
      "resource_bundles": {
        "MNSwiftKit_UI": [
          "MNSwiftKit/UI/Resources/**/*.{png,jpg,jpeg}"
        ]
      },
      "dependencies": {
        "MNSwiftKit/Layout": [],
        "MNSwiftKit/Extension": []
      }
    },
    {
      "name": "Utility",
      "frameworks": [
        "UIKit",
        "Foundation",
        "AVFoundation",
        "CoreFoundation",
        "CoreGraphics",
        "CoreMedia",
        "Photos",
        "AdSupport",
        "AppTrackingTransparency"
      ],
      "source_files": "MNSwiftKit/Utility/Sources/**/*.swift"
    },
    {
      "name": "Toast",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Toast/Sources/**/*.swift",
      "resource_bundles": {
        "MNSwiftKit_Toast": [
          "MNSwiftKit/Toast/Resources/*.{png,jpg,jpeg}"
        ]
      }
    },
    {
      "name": "Slider",
      "frameworks": [
        "UIKit",
        "Foundation",
        "QuartzCore",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Slider/Sources/**/*.swift"
    },
    {
      "name": "Player",
      "frameworks": [
        "UIKit",
        "Foundation",
        "AVFoundation",
        "AudioToolbox"
      ],
      "source_files": "MNSwiftKit/Player/Sources/**/*.swift"
    },
    {
      "name": "Spliter",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Spliter/Sources/**/*.swift",
      "dependencies": {
        "MNSwiftKit/Layout": []
      }
    },
    {
      "name": "Layout",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Layout/Sources/**/*.swift"
    },
    {
      "name": "Refresh",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Refresh/Sources/**/*.swift"
    },
    {
      "name": "Exporter",
      "frameworks": [
        "Foundation",
        "AVFoundation",
        "QuartzCore"
      ],
      "source_files": "MNSwiftKit/Exporter/Sources/**/*.swift"
    },
    {
      "name": "Purchase",
      "frameworks": [
        "UIKit",
        "Foundation",
        "StoreKit"
      ],
      "source_files": "MNSwiftKit/Purchase/Sources/**/*.swift"
    },
    {
      "name": "Database",
      "frameworks": [
        "Foundation",
        "AVFoundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Database/Sources/**/*.swift"
    },
    {
      "name": "Definition",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Definition/Sources/**/*.swift"
    },
    {
      "name": "Extension",
      "frameworks": [
        "Foundation",
        "CoreFoundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Extension/Sources/**/*.swift",
      "dependencies": {
        "MNSwiftKit/AnimatedImage": []
      }
    },
    {
      "name": "Controller",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics",
        "WebKit"
      ],
      "source_files": "MNSwiftKit/Controller/Sources/**/*.swift",
      "resource_bundles": {
        "MNSwiftKit_Controller": [
          "MNSwiftKit/Controller/Resources/*.{png,jpg,jpeg}"
        ]
      },
      "dependencies": {
        "MNSwiftKit/Toast": [],
        "MNSwiftKit/Layout": [],
        "MNSwiftKit/Refresh": [],
        "MNSwiftKit/Definition": [],
        "MNSwiftKit/EmptyView": [],
        "MNSwiftKit/Transitioning": [],
        "MNSwiftKit/HTTPRequest": []
      }
    },
    {
      "name": "EmptyView",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/EmptyView/Sources/**/*.swift"
    },
    {
      "name": "Networking",
      "frameworks": [
        "Foundation",
        "SystemConfiguration",
        "CoreTelephony",
        "CoreAudio",
        "Security",
        "CoreFoundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Networking/Sources/**/*.swift",
      "resource_bundles": {
        "MNSwiftKit_Networking": [
          "MNSwiftKit/Networking/Resources/*.json"
        ]
      }
    },
    {
      "name": "AssetPicker",
      "frameworks": [
        "UIKit",
        "Foundation",
        "AVFoundation",
        "Photos",
        "CoreFoundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/AssetPicker/Sources/**/*.swift",
      "resource_bundles": {
        "MNSwiftKit_AssetPicker": [
          "MNSwiftKit/AssetPicker/Resources/**/*.{png,jpg,jpeg}"
        ]
      },
      "dependencies": {
        "MNSwiftKit/Toast": [],
        "MNSwiftKit/Slider": [],
        "MNSwiftKit/Player": [],
        "MNSwiftKit/Layout": [],
        "MNSwiftKit/Refresh": [],
        "MNSwiftKit/Exporter": [],
        "MNSwiftKit/Definition": [],
        "MNSwiftKit/EmptyView": [],
        "MNSwiftKit/AnimatedImage": []
      }
    },
    {
      "name": "Transitioning",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Transitioning/Sources/**/*.swift"
    },
    {
      "name": "HTTPRequest",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/HTTPRequest/Sources/**/*.swift",
      "dependencies": {
        "MNSwiftKit/Networking": []
      }
    },
    {
      "name": "AnimatedImage",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics",
        "ImageIO",
        "CoreServices",
        "UniformTypeIdentifiers"
      ],
      "source_files": "MNSwiftKit/AnimatedImage/Sources/**/*.swift"
    },
    {
      "name": "Full",
      "dependencies": {
        "MNSwiftKit/UI": [],
        "MNSwiftKit/Toast": [],
        "MNSwiftKit/Slider": [],
        "MNSwiftKit/Utility": [],
        "MNSwiftKit/Player": [],
        "MNSwiftKit/Spliter": [],
        "MNSwiftKit/Layout": [],
        "MNSwiftKit/Refresh": [],
        "MNSwiftKit/Exporter": [],
        "MNSwiftKit/Purchase": [],
        "MNSwiftKit/Database": [],
        "MNSwiftKit/Definition": [],
        "MNSwiftKit/Extension": [],
        "MNSwiftKit/Controller": [],
        "MNSwiftKit/EmptyView": [],
        "MNSwiftKit/Networking": [],
        "MNSwiftKit/AssetPicker": [],
        "MNSwiftKit/Transitioning": [],
        "MNSwiftKit/HTTPRequest": [],
        "MNSwiftKit/AnimatedImage": []
      }
    }
  ],
  "swift_version": "5.0"
}
