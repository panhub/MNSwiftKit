{
  "name": "MNSwiftKit",
  "version": "0.1.0",
  "summary": "Swift开发基础组件。",
  "description": "Swift开发基础组件，模块化。",
  "homepage": "https://github.com/panhub/MNSwiftKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "panhub": "fengpann@163.com"
  },
  "source": {
    "git": "https://github.com/panhub/MNSwiftKit.git",
    "tag": "0.1.0"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "9.0"
  },
  "default_subspecs": "Full",
  "subspecs": [
    {
      "name": "Core",
      "frameworks": [
        "Foundation",
        "CoreFoundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Core/Sources/*.swift"
    },
    {
      "name": "Base",
      "frameworks": [
        "UIKit",
        "Foundation",
        "AVFoundation",
        "CoreGraphics",
        "QuartzCore",
        "ImageIO",
        "CoreServices",
        "UniformTypeIdentifiers"
      ],
      "source_files": "MNSwiftKit/Base/Sources/**/*.swift",
      "dependencies": {
        "MNSwiftKit/Layout": []
      }
    },
    {
      "name": "Unity",
      "frameworks": [
        "UIKit",
        "Foundation",
        "AVFoundation",
        "CoreFoundation",
        "CoreGraphics",
        "CoreMedia",
        "Photos",
        "AdSupport",
        "AppTrackingTransparency"
      ],
      "source_files": "MNSwiftKit/Unity/Sources/*.swift"
    },
    {
      "name": "Toast",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Toast/Sources/*.swift",
      "resource_bundles": {
        "MNSwiftKit_Toast": [
          "MNSwiftKit/Toast/Resources/*.{png,jpg,jpeg}"
        ]
      }
    },
    {
      "name": "Layout",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Layout/Sources/*.swift"
    },
    {
      "name": "Refresh",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": [
        "MNSwiftKit/Refresh/Sources/*.swift",
        "MNSwiftKit/Refresh/Sources/**/*.swift"
      ]
    },
    {
      "name": "Purchase",
      "frameworks": [
        "UIKit",
        "Foundation",
        "StoreKit"
      ],
      "source_files": "MNSwiftKit/Purchase/Sources/*.swift"
    },
    {
      "name": "Database",
      "frameworks": [
        "Foundation",
        "AVFoundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Database/Sources/*.swift"
    },
    {
      "name": "EmptyView",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/EmptyView/Sources/*.swift"
    },
    {
      "name": "Networking",
      "frameworks": [
        "Foundation",
        "SystemConfiguration",
        "CoreTelephony",
        "CoreAudio",
        "Security",
        "CoreFoundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/Networking/Sources/*.swift",
      "resource_bundles": {
        "MNSwiftKit_Networking": [
          "MNSwiftKit/Networking/Resources/*.json"
        ]
      }
    },
    {
      "name": "HTTPRequest",
      "frameworks": [
        "UIKit",
        "Foundation",
        "CoreGraphics"
      ],
      "source_files": "MNSwiftKit/HTTPRequest/Sources/*.swift",
      "dependencies": {
        "MNSwiftKit/Networking": []
      }
    },
    {
      "name": "Full",
      "dependencies": {
        "MNSwiftKit/Core": [],
        "MNSwiftKit/Base": [],
        "MNSwiftKit/Unity": [],
        "MNSwiftKit/Toast": [],
        "MNSwiftKit/Layout": [],
        "MNSwiftKit/Refresh": [],
        "MNSwiftKit/Purchase": [],
        "MNSwiftKit/Database": [],
        "MNSwiftKit/EmptyView": [],
        "MNSwiftKit/Networking": [],
        "MNSwiftKit/HTTPRequest": []
      }
    }
  ],
  "swift_version": "5.0"
}
